      <div class="content">
        <div class="container-fluid">
          <!-- your content here -->
          <form name="formEditBook" method="POST" action="/tables/update/{{book._id}}" enctype="multipart/form-data">
            <div class="form_edit row">
              <div class="label col-2">
                <label for="bookTitle">Title: </label>
                <label for="bookPrice">Price: </label>
                <label for="bookPrice">Base Price: </label>
                <label for="coverFirst">First Cover: </label>
                <label for="coverSecond">Second Cover: </label>
              </div>

              <div class="input col-10">
                {{!-- input Title --}}
                <input type="text" required name="title">
                {{!-- input Price --}}
                <input type="text" required name="price">
                {{!-- input Base Price --}}
                <input type="text" required name="oldPrice">
                {{!-- input First Cover --}}
                <input type="text" required name="coverFirst">
                {{!-- input Second Cover --}}
                <input type="text" required name="coverSecond">
              </div>
            </div>
            <div class="form-group">
              <a href="/" class="btn btn-warning">Cancel</a>
              <button type="submit" class="btn btn-primary">Insert</button>
            </div>

          </form>
        </div>
